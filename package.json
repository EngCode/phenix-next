{
  "name": "pds-remix",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "remix dev --manual",
    "build": "remix build",
    "start": "remix-serve build",
    "phenix-js": "npx webpack --config scripts.webpack.js --watch",
    "phenix-sass": "npx node-sass --watch pds/sass -o app/css --output-style compressed --source-map true",
    "animate-sass": "npx node-sass --watch pds/animations/ -o app/css/animations/ --output-style compressed"
  },
  "dependencies": {
    "@remix-run/node": "^2.3.1",
    "@remix-run/react": "^2.3.1",
    "@remix-run/serve": "^2.3.1",
    "isbot": "^3.6.13",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sort-by": "^0.0.2",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.3.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.1.6",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
